FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "0.0001";
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];
oriented        oriented;


internalField   uniform 0;

boundaryField
{
    rim
    {
        type            calculated;
        value           uniform 0;
    }
}


