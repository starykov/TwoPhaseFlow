FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.0001";
    object      recon::centre;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    rim
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}


